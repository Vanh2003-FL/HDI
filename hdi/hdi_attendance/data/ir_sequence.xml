<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequence for Attendance Exception -->
        <record id="seq_attendance_exception" model="ir.sequence">
            <field name="name">Attendance Exception</field>
            <field name="code">attendance.exception</field>
            <field name="prefix">AE</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Attendance Explanation -->
        <record id="seq_attendance_explanation" model="ir.sequence">
            <field name="name">Attendance Explanation</field>
            <field name="code">attendance.explanation</field>
            <field name="prefix">AX</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Work Location -->
        <record id="seq_work_location" model="ir.sequence">
            <field name="name">Work Location</field>
            <field name="code">hr.work.location</field>
            <field name="prefix">WL</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>