<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Stock Batch Split -->
        <record id="seq_stock_batch_split" model="ir.sequence">
            <field name="name">Stock Batch Split</field>
            <field name="code">stock.batch.split</field>
            <field name="prefix">SPLIT/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Stock Batch Merge -->
        <record id="seq_stock_batch_merge" model="ir.sequence">
            <field name="name">Stock Batch Merge</field>
            <field name="code">stock.batch.merge</field>
            <field name="prefix">MERGE/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
