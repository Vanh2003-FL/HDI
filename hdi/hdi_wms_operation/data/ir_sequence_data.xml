<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequence for Receipt Operation -->
    <record id="seq_receipt_operation" model="ir.sequence">
        <field name="name">Receipt Operation</field>
        <field name="code">receipt.operation</field>
        <field name="prefix">NK/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequence for Transfer Operation -->
    <record id="seq_transfer_operation" model="ir.sequence">
        <field name="name">Transfer Operation</field>
        <field name="code">transfer.operation</field>
        <field name="prefix">DC/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequence for Dispatch Operation -->
    <record id="seq_dispatch_operation" model="ir.sequence">
        <field name="name">Dispatch Operation</field>
        <field name="code">dispatch.operation</field>
        <field name="prefix">XK/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequence for Barcode Item List -->
    <record id="seq_barcode_item_list" model="ir.sequence">
        <field name="name">Barcode Item List</field>
        <field name="code">barcode.item.list</field>
        <field name="prefix">BK/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
