<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="wms_receipt_line_view_tree" model="ir.ui.view">
        <field name="name">wms.receipt.line.view.list</field>
        <field name="model">wms.receipt.line</field>
        <field name="arch" type="xml">
            <list string="Receipt Lines">
                <field name="receipt_id"/>
                <field name="product_id"/>
                <field name="expected_qty"/>
                <field name="received_qty"/>
                <field name="putaway_location_id"/>
                <field name="quality_status" widget="badge"/>
            </list>
        </field>
    </record>
</odoo>
