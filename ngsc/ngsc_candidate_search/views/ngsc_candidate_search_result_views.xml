<odoo>
   <record id="view_candidate_search_result_tree" model="ir.ui.view">
        <field name="name">view_candidate_search_result_tree</field>
        <field name="model">ngsc.candidate.search.result</field>
        <field name="arch" type="xml">
            <list>
                <header>
                    <button name="action_compair" string="So sánh" type="object"
                            class="btn-primary candidate_compare_button"/>
                    <button name="action_export_excel" string=" Xuất" type="object"
                            class="btn-primary fa fa-download"/>
                </header>
                <field name="en_type_id"/>
                <field name="state_hr_employee"/>
                <field name="barcode"/>
                <field name="hr_employee_id"/>
                <field name="job_title"/>
                <field name="work_email"/>
                <field name="en_area_id"/>
                <field name="en_block_id"/>
                <field name="department_id"/>
                <field name="en_department_id"/>
                <field name="weight" widget="custom_weight" options="{'no_open': True}"/>
                <field name="en_status_hr"/>
            </list>
        </field>
    </record>
    <record id="view_candidate_search_result_tree_search" model="ir.ui.view">
        <field name="name">view_candidate_search_result_tree_search</field>
        <field name="model">ngsc.candidate.search.result</field>
        <field name="arch" type="xml">
            <search string="">
                <field name="en_type_id"/>
                <field name="barcode"/>
                <field name="hr_employee_id"/>
                <field name="job_title"/>
                <field name="work_email"/>
                <field name="en_area_id"/>
                <field name="en_block_id"/>
                <field name="department_id"/>
                <field name="en_department_id"/>
                <field name="en_status_hr"/>
            </search>
       </field>
    </record>
</odoo>