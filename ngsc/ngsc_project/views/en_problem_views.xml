<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ngsc_project_en_problem_view_form" model="ir.ui.view">
            <field name="name">ngsc.project.en.problem.view.form</field>
            <field name="model">en.problem</field>
            <field name="inherit_id" ref="ngsd_base.problem_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='inf_expand_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='tic_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>