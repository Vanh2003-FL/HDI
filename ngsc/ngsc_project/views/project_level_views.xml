<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="en_project_level_view_tree" model="ir.ui.view">
            <field name="name">en.project.level.view.tree</field>
            <field name="model">en.project.level</field>
            <field name="arch" type="xml">
                <list string="Cấp độ dự án" editable="bottom" sample="1">
                    <field name="name"/>
                    <field name="description"/>
                </list>
            </field>
        </record>

        <record id="en_project_level_act_window" model="ir.actions.act_window">
            <field name="name">Cấp độ dự án</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">en.project.level</field>
            <field name="view_mode">list</field>
        </record>
    </data>
</odoo>