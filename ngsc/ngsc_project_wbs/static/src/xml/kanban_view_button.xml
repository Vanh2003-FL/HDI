<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-extend="KanbanView.buttons">
        <t t-jquery="button" t-operation="after">
            <t t-if="widget &amp;&amp; widget.modelName == 'project.project'">
                <button class="btn btn-primary o_list_import_project_task"
                        type="button"
                        t-on-click="_onImportProjectTask">
                    <i class="fa fa-upload"/> Nhập công việc
                </button>
                <button class="btn btn-secondary o_list_button_download_import_task_template"
                        type="button"
                        t-on-click="_onImportProjectTask">
                    <i class="fa fa-download"/> Tải file mẫu
                </button>
            </t>
        </t>
    </t>
</templates>