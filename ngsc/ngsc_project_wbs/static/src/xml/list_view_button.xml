<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-extend="ListView.buttons">
        <t t-jquery="button.o_list_export_xlsx" t-operation="after">
            <button t-if="widget.modelName == 'en.nonproject.task'" class="btn btn-primary o_list_import_nonproject_task" type="button">
                <i class="fa fa-upload"/> Nhập công việc
            </button>
        </t>
        <t t-jquery="button.o_list_import_nonproject_task" t-operation="after">
            <button t-if="widget.modelName == 'en.nonproject.task'" class="btn btn-secondary o_list_button_download_nonproject_task_template" type="button">
                <i class="fa fa-download"/> Tải file mẫu
            </button>
        </t>
    </t>
</templates>