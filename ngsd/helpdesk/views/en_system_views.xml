<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="en_system_view_tree" model="ir.ui.view">
           <field name="name">en.system.tree</field>
           <field name="model">en.system</field>
           <field name="arch" type="xml">
               <list editable="bottom">
                   <field name="name"/>
               </list>
           </field>
        </record>

    <record id="en_system_action" model="ir.actions.act_window">
        <field name="res_model">en.system</field>
        <field name="view_mode">list</field>
    </record>
    <menuitem id="en_system_menu" action="en_system_action"  sequence="7" parent="helpdesk.helpdesk_menu_config"/>

    </data>
</odoo>
