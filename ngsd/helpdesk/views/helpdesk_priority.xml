<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="helpdesk_priority_view_tree" model="ir.ui.view">
        <field name="name">helpdesk_priority_view_tree</field>
        <field name="model">helpdesk.priority</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
                <field name="en_priority"/>
                <field name="en_project_apply_ids" widget="many2many_tags"/>
                <field name="en_progress_working_hours"/>
                <field name="en_rule_default"/>
                <field name="en_time_rep"/>
                <field name="en_time_action"/>
            </list>
        </field>
    </record>
    <record id="helpdesk_priority_action" model="ir.actions.act_window">
        <field name="res_model">helpdesk.priority</field>
        <field name="view_mode">list</field>
    </record>
    <menuitem id="helpdesk_priority_menu" action="helpdesk_priority_action"  sequence="2" parent="helpdesk.helpdesk_menu_config"/>

</odoo>
