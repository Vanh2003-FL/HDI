<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="module_category_room" model="ir.module.category">
        <field name="name">Room</field>
        <field name="description">Enable the user to manage meeting rooms available for booking</field>
        <field name="sequence">15</field>
    </record>

    <record id="group_room_manager" model="res.groups">
        <field name="name">Manage Rooms</field>
        <field name="category_id" ref="module_category_room"/>
        <field name="implied_ids" eval="[Command.link(ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</odoo>
