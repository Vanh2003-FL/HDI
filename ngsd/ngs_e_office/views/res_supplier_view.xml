<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_supplier_tree" model="ir.ui.view">
            <field name="name">res.supplier.tree</field>
            <field name="model">res.supplier</field>
            <field name="arch" type="xml">
                <list editable="top">
                    <field name="name"/>
                </list>
            </field>
        </record>

         <record id="res_supplier_action" model="ir.actions.act_window">
            <field name="name">Danh sách nhà cung cấp</field>
            <field name="res_model">res.supplier</field>
            <field name="view_mode">list</field>
        </record>
    </data>
</odoo>
