<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="get_project_resource_data_refresh_data_cron" model="ir.cron">
        <field name="name">Cập nhật dữ liệu powerBI</field>
        <field name="model_id" ref="ngs_powerbi.model_get_project_resource_data"/>
        <field name="state">code</field>
        <field name="code">model.refresh_data()</field>
        <field name='interval_number'>30</field>
        <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 17:05:00')"/>
        <field name='interval_type'>minutes</field>
        <!-- removed numbercall: not supported in Odoo 18 -->
        <field name="active">0</field>
    </record>
