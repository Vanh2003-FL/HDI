<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_id_technical" model="ir.sequence">
        <field name="name">Sinh mã</field>
        <field name="code">seq.id</field>
        <field name="prefix"></field>
        <field name="padding">6</field>
        <field name="active" eval="True"/>
        <field name="company_id" eval="False"/>
    </record>
    <record id="quality_seq_id_technical" model="ir.sequence">
        <field name="name">Sinh mã</field>
        <field name="code">quality.seq.id</field>
        <field name="prefix"></field>
        <field name="padding">6</field>
        <field name="active" eval="True"/>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_code_partner_ac" model="ir.sequence">
        <field name="name">Sinh mã AC</field>
        <field name="code">seq.code.partner.ac</field>
        <field name="prefix">AC</field>
        <field name="padding">4</field>
        <field name="active" eval="True"/>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_res_partner" model="ir.sequence">
        <field name="name">Seq Res Partner</field>
        <field name="code">ngsd.base.res.partner</field>
        <field name="prefix">KH</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_crm_lead_code" model="ir.sequence">
        <field name="name">Seq Res Partner</field>
        <field name="code">ngsd.base.crm.lead</field>
        <field name="prefix">CH</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_ot_plan_code" model="ir.sequence">
        <field name="name">Seq OT Plan code</field>
        <field name="code">ngsd.base.ot.plan.code</field>
        <field name="prefix">OT</field>
        <field name="padding">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_code_borrow_employee" model="ir.sequence">
            <field name="name">Mã mượn nhân sự</field>
            <field name="code">code.borrow.employee</field>
            <field name="prefix">MN</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="active" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

    <record id="seq_code_lender_employee" model="ir.sequence">
            <field name="name">Mã danh sách mượn</field>
            <field name="code">code.lender.employee</field>
            <field name="prefix">CM</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="active" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>
