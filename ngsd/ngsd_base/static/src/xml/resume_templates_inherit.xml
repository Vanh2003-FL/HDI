<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-extend="hr_resume_data_row">
        <t t-jquery="p" t-operation="before">
            <t t-if="data.hr_resume_type == 'exp'">
                <p t-if="data.name_project">
                    Tên dự án: <span class="o_resume_line_desc" t-esc="data.name_project"/>
                </p>
                 <p t-if="data.domain_project">
                     Domain dự án: <span class="o_resume_line_desc" t-esc="data.domain_project"/>
                </p>
                 <p t-if="data.service_provided">
                     Dịch vụ cung cấp: <span class="o_resume_line_desc" t-esc="data.service_provided"/>
                </p>
                 <p t-if="data.position_responsibility">
                     Vị trí đảm nhiệm: <span class="o_resume_line_desc" t-esc="data.position_responsibility"/>
                </p>
            </t>
        </t>
    </t>
</templates>
