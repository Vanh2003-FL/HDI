<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       <record id="email_data_tree" model="ir.ui.view">
           <field name="name">email_data_tree</field>
           <field name="model">email.data</field>
           <field name="arch" type="xml">
               <list string="">
                   <field name="id"/>
                   <field name="lead_id"/>
                   <field name="tmp"/>
                   <field name="email_to"/>
                   <field name="ctx"/>
                   <field name="id_mess"/>
                   <field name="state"/>
               </list>
           </field>
       </record>

        <record id="email_data_form" model="ir.ui.view">
            <field name="name">email_data_form</field>
            <field name="model">email.data</field>
            <field name="arch" type="xml">
                 <form>
                    <sheet>
                        <group>
                            <group>
                               <field name="lead_id"/>
                               <field name="tmp"/>
                               <field name="email_to"/>
                               <field name="ctx"/>
                               <field name="id_mess"/>
                               <field name="state"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="email_data_action" model="ir.actions.act_window">
            <field name="name">Email Data</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">email.data</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>