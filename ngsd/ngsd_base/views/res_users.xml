<odoo>
    <record id="ngsd_res_users_view_form" model="ir.ui.view">
        <field name="name">ngsd_res_users_view_form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="hr_holidays.res_users_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='%(hr_holidays.hr_leave_allocation_action_my)d']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
        </field>
    </record>

    <record id="table_criteria_api_view_tree" model="ir.ui.view">
        <field name="name">table.criteria.api.tree</field>
        <field name="model">table.criteria.api</field>
        <field name="arch" type="xml">
            <list string="" editable="bottom">
                <field name="criteria_lv1"/>
                <field name="criteria_lv2"/>
            </list>
        </field>
    </record>

    <record id="table_criteria_api_view_action" model="ir.actions.act_window">
        <field name="name">Bảng cài tiêu chí</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">table.criteria.api</field>
        <field name="view_mode">list</field>
    </record>

</odoo>
