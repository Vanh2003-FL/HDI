<?xml version="1.0"?>
<odoo>
        <record id="change_lock_license_form" model="ir.ui.view">
            <field name="name">change_lock_license_form</field>
            <field name="model">change.lock.license</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="employee_ids" invisible="1"/>
                                <field name="type" invisible="1"/>
                                <field name="date_lock"/>
                                <field name="date_expire"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="button_confirm" string="Xác nhận" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


</odoo>
