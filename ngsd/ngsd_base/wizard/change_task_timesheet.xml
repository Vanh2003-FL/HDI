<?xml version="1.0"?>
<odoo>
        <record id="change_task_timesheet_form" model="ir.ui.view">
            <field name="name">change_task_timesheet_form</field>
            <field name="model">change.task.timesheet</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="timesheet_ids"  invisible="1"/>
                                <field name="project_id"/>
                                <field name="task_id"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="button_confirm" string="Xác nhận" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


</odoo>
