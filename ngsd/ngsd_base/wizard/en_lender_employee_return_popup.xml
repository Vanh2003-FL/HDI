<?xml version="1.0"?>
<odoo>
        <record id="en_lender_employee_return_popup_form" model="ir.ui.view">
            <field name="name">en_lender_employee_return_popup_form</field>
            <field name="model">en.lender.employee.return.popup</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="detail_id"  invisible="1"/>
                                <field name="date_return"/>
                                <field name="date_end"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="button_confirm" string="Xác nhận" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


</odoo>
