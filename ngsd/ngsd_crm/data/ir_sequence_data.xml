<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_code_lead" model="ir.sequence">
            <field name="name">Sinh mã Mã/ID Lead</field>
            <field name="code">seq.code.lead</field>
            <field name="prefix">CH</field>
            <field name="padding">4</field>
            <field name="active" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_x_sale_contract" model="ir.sequence">
            <field name="name">Mã</field>
            <field name="code">sale.contract.code</field>
            <field name="prefix">HĐ</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="active" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
