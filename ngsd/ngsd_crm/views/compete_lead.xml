<odoo>

    <record id="compete_lead_view_tree" model="ir.ui.view">
        <field name="name">compete_lead_view_tree</field>
        <field name="model">compete.lead</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
                <field name="rate"/>
            </list>
        </field>
    </record>

    <record id="compete_lead_action" model="ir.actions.act_window">
        <field name="name">Tính cạnh tranh</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">compete.lead</field>
        <field name="view_mode">list</field>
    </record>
</odoo>
