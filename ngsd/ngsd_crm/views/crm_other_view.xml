<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="crm_support_state_view_tree" model="ir.ui.view">
            <field name="name">crm.support.state.view.tree</field>
            <field name="model">crm.support.state</field>
            <field name="arch" type="xml">
                <list editable="bottom">
                    <field name="name"/>
                </list>
            </field>
        </record>

        <record id="crm_support_state_action" model="ir.actions.act_window">
            <field name="name">Tình trạng support</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">crm.support.state</field>
            <field name="view_mode">list</field>
        </record>

        <record id="crm_contact_role_view_tree" model="ir.ui.view">
            <field name="name">crm.contact.role.view.tree</field>
            <field name="model">crm.contact.role</field>
            <field name="arch" type="xml">
                <list editable="bottom">
                    <field name="name"/>
                </list>
            </field>
        </record>

        <record id="crm_contact_role_action" model="ir.actions.act_window">
            <field name="name">Vai trò đầu mối liên lạc</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">crm.contact.role</field>
            <field name="view_mode">list</field>
        </record>
    </data>
</odoo>
