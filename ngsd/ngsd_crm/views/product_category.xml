<odoo>
    <record id="ngsd_product_category_form_view" model="ir.ui.view">
        <field name="name">ngsd_product_category_form_view</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='parent_id']" position="after">
                <field name="product_type_id"  class="oe_inline"/>
            </xpath>
        </field>
    </record>
</odoo>
